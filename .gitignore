# Repo-level .gitignore for secure-api-gateway
# Ignore node modules and build artifacts
node_modules/
/.pnp/

# Next.js build output
.next/
out/

# TypeScript
dist/
build/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Local env files (do not commit secrets)
.env
.env.*

# OS files
.DS_Store
Thumbs.db

# IDE/editor files
.vscode/
.idea/
*.sublime-workspace
*.sublime-project

# Coverage reports
coverage/
.nyc_output/

# Docker
docker-compose.override.yml
docker-compose.local.yml

# Python virtual envs and caches (ml-service)
venv/
.venv/
env/
__pycache__/
*.py[cod]
pip-wheel-metadata/
.pytest_cache/

# npm package lock should be committed, do not ignore package-lock.json or pnpm-lock.yaml by default

# Misc
dist-debug/
.cache/
tmp/
node_modules/
